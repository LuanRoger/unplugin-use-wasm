{
  "name": "vite-plugin-use-wasm",
  "version": "1.0.0",
  "description": "Support to \"use wasm\" directive to compile to WebAssembly",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/LuanRoger/vite-plugin-use-wasm.git"
  },
  "homepage": "https://github.com/LuanRoger/vite-plugin-use-wasm",
  "bugs": "https://github.com/LuanRoger/vite-plugin-use-wasm/issues",
  "keywords": [
    "plugin",
    "vite-plugin",
    "webassembly",
    "wasm",
    "wast",
    "emscripten",
    "assemblyscript",
    "typescript",
    "directive"
  ],
  "author": "Luan Roger <luan.roger.2003@gmail.com> (https://github.com/LuanRoger)",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    "types": "./dist/index.d.mts",
    "import": "./dist/index.mjs",
    "default": "./dist/index.cjs"
  },
  "files": [
    "dist",
    "LICENSE",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "build": "tsdown --config tsdown.config.ts",
    "test": "vitest ./tests --run",
    "publish": "pnpm run build && npm publish --access public",
    "changelog:gen": "git-cliff --config ./scripts/git-cliff.toml --output CHANGELOG.md"
  },
  "peerDependencies": {
    "vite": ">=7.0.0"
  },
  "dependencies": {
    "assemblyscript": "^0.28.6"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "fs-extra": "^11.3.1",
    "git-cliff": "^2.10.0",
    "tsdown": "^0.15.0",
    "vite": "^7.1.5",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.15.0"
}
